#include<iostream>
#include<string>
using namespace std;
int main()
{
	int t,n;
	string y;
	cin>>t;
	for(int i=0;i<t;i++)
	{
		cin>>n;
		cin>>y;
		string out;
		for(int i=0;i<y.size();i++)
		{

			if(y[i]=='S')
				out+= 'E';
			else
				out+= 'S';
		}
		cout<<"Case #"<<i+1<<": "<<out<<endl;
	}
	return 0;
}
